%{
#include "calc.tab.h"  // Include the header generated by Bison
%}

%%
[0-9]+          { yylval = atoi(yytext); return NUMBER; }
[ \t\n]+        ;  // Skip whitespaces
"+"             { return PLUS; }
"*"             { return MULT; }
.               { return yytext[0]; } // Catch all other single characters
%%

int yywrap() {
    return 1;
}
